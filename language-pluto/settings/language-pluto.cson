    'increaseIndentPattern': '(:|=|=:|::|<:|>:|\\\\|\\[[^\\]]*|\\{[^\\}]*|\\([^\\)]*)\\s*$'
    'decreaseIndentPattern': '^(\\s*(elsif|elif|else|catch|ensure)\\b.*:|\\s+\\.{2}|\\s+\\]|\\s+\\}|\\s*\\}|\\s*\\]|\\s*\\))\\s*$' # while
